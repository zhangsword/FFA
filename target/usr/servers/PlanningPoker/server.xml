<server description="new server">

    <!-- Enable features -->
    <featureManager>
    	<feature>websocket-1.0</feature>
    </featureManager>

    <!-- To access this server from a remote client add a host attribute to the following element, e.g. host="*" -->
    <httpEndpoint httpPort="80" httpsPort="443" host="*" id="defaultHttpEndpoint"/>

    <webApplication id="PlanningPoker" location="PlanningPoker.war" name="PlanningPoker"/>
</server>
